    <validationRules>
        <fullName>MT_AssetMatch_CampType_vs_OppApp</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
  ISBLANK(MT_OpportunityApproval_ref__r.MT_Asset_ref__c),
  TEXT(MT_OpportunityApproval_ref__r.P7S1_internerPartner__c) &lt;&gt; MT_MediaCampaignType_ref__r.MT_Asset_ref__r.Name,
  MT_OpportunityApproval_ref__r.MT_Asset_ref__c &lt;&gt; MT_MediaCampaignType_ref__r.MT_Asset_ref__c
)</errorConditionFormula>
        <errorDisplayField>MT_MediaCampaignType_ref__c</errorDisplayField>
        <errorMessage>Der Kampagnentyp muss zum gleichen Asset gehören wie der interne Partner auf der Opportunity Freigabe.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_CampaignInContract</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
NOT ( ISBLANK ( MT_ContractStartdate__c ) ),
NOT ( ISBLANK ( MT_ContractEnddate__c ) ),
OR(
MT_OpportunityApproval_ref__r.P7S1_OppKampagnenstart__c &lt; MT_ContractStartdate__c ,
MT_OpportunityApproval_ref__r.P7S1_OppKampagnenende__c &gt; MT_ContractEnddate__c
)
)</errorConditionFormula>
        <errorMessage>Achtung Kampagnenlaufzeit muss innerhalb Vertragslaufzeit sein.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_CheckIfCampaignOppAppIsApproved</fullName>
        <active>true</active>
        <description>Only allow the checkbox to be checked if Campaign Status is &quot;Campaign approved&quot; AND OpportunityApproval ApprovalState is &quot;Genehmigt&quot;</description>
        <errorConditionFormula>IF(AND(
ISPICKVAL(MT_Status__c,&quot;Campaign approved&quot;),
ISPICKVAL(MT_OpportunityApproval_ref__r.P7S1_ApprovalState__c ,&quot;Genehmigt&quot;)),
FALSE,IF(MT_IsContractSigned__c,NOT(ISCHANGED(MT_Status__c)),FALSE)
)</errorConditionFormula>
        <errorDisplayField>MT_IsContractSigned__c</errorDisplayField>
        <errorMessage>Sowohl die Kampagne als auch die Opportunity Freigabe muss genehmigt sein um dieses Feld zu ändern.</errorMessage>
    </validationRules>
