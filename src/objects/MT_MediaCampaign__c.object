        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>MT_BMVDooH__c</fullName>
        <description>Das BMV DooH, das in dieser Kampagne ausgestrahlt werden soll.</description>
        <externalId>false</externalId>
        <label>BMV DooH</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>MT_BarterYield__c</fullName>
        <description>Der ermittelte Barter-Yield für diese Kampagne (Netto-Barter-Volumen / BMV TV)</description>
        <externalId>false</externalId>
        <formula>IF(MT_BmvTv__c = 0, 0, MT_NetBarterVolume__c / MT_BmvTv__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Barter Yield</label>
        <precision>18</precision>
        <required>false</required>
        <scale>3</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
