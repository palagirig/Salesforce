    <fields>
        <fullName>MT_ActualMonthlyBMV_sum__c</fullName>
        <description>Die Summe des über die Monat(splän)e ausgestrahlten Volumens.</description>
        <externalId>false</externalId>
        <label>Sum of BMV Monthly Plan</label>
        <summarizedField>MT_MediaCampaignMonthlyPlan__c.MT_SumBMV__c</summarizedField>
