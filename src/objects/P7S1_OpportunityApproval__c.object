        <fullName>MT_Shifting_Valid</fullName>
        <active>true</active>
        <errorConditionFormula>(ISBLANK(MT_MonthlyShiftingRight_Asset__c) == false) &amp;&amp; (MT_MonthlyShiftingRight_Asset__c &gt; 1 || MT_MonthlyShiftingRight_Asset__c &lt; 0)</errorConditionFormula>
