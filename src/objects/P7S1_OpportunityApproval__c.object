    <validationRules>
        <fullName>MT_CampaignInContract</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
NOT ( ISBLANK ( MT_ContractStartdate__c ) ),
NOT ( ISBLANK ( MT_ContractEnddate__c ) ),
OR(
P7S1_OppKampagnenstart__c &lt; MT_ContractStartdate__c ,
P7S1_OppKampagnenende__c &gt; MT_ContractEnddate__c
)
)</errorConditionFormula>
        <errorMessage>Achtung Kampagnenlaufzeit muss innerhalb Vertragslaufzeit sein.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_InternalPartnerMatchAsset</fullName>
        <active>true</active>
        <description>Check to see if Internal Partner and Internal Partner Asset match otherwise throw error</description>
        <errorConditionFormula>AND(
NOT( ISBLANK( MT_Asset_ref__r.Name ) ),
UPPER( TRIM( IF( ISPICKVAL( P7S1_internerPartner__c, &quot;sonstiger&quot; ), P7S1_sonstigerInternerPartner__c, Text( P7S1_internerPartner__c ) ) ) ) &lt;&gt; UPPER( TRIM( MT_Asset_ref__r.Name ) )
)</errorConditionFormula>
        <errorMessage>Interner Partner und interner Partner (Asset) muss gleich sein</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_PreventFixedMonthlyPaymentIfDisabled</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
    NOT ( ISBLANK ( MT_FixRevenue_MonthlyPayment__c ) ) ,
    NOT ( ISPICKVAL( MT_FixRevenue_Type__c , &apos;Payment&apos; ) )
  )</errorConditionFormula>
        <errorDisplayField>MT_FixRevenue_MonthlyPayment__c</errorDisplayField>
        <errorMessage>Eine Summe darf nur angegeben werden, wenn fixe Zahlung ausgewählt ist.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_RequiredValueForFixedMonthlyPayment</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
    ISBLANK ( MT_FixRevenue_MonthlyPayment__c  ),
    ISPICKVAL( MT_FixRevenue_Type__c , &apos;Payment&apos; )
  )</errorConditionFormula>
        <errorDisplayField>MT_FixRevenue_MonthlyPayment__c</errorDisplayField>
        <errorMessage>Wenn fixe Zahlung ausgewählt ist muss eine Summe angegeben werden.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_SLMDifferenceFieldRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(MT_SLMAfterYield__c) &lt;&gt; TEXT(MT_SLMAccordingToContract__c), ISBLANK( MT_SLMDifference__c ))</errorConditionFormula>
        <errorDisplayField>MT_SLMDifference__c</errorDisplayField>
        <errorMessage>Dieses Feld ist ein Pflichtfeld, bitte tragen Sie etwas ein.</errorMessage>
    </validationRules>
