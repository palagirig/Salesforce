    <validationRules>
        <fullName>MT_BC_ImportData_EmptyBrutVolume</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISBLANK(MT_MB1__c), ISNULL(MT_MB1__c)), OR(ISBLANK(MT_MB3__c), ISNULL(MT_MB3__c)))</errorConditionFormula>
        <errorMessage>Bitte einen Wert &gt;=0 in MB1 oder MB3 eintragen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_EmptyCampaignId</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(MT_CampaignID__c), ISNULL(MT_CampaignID__c))</errorConditionFormula>
        <errorDisplayField>MT_CampaignID__c</errorDisplayField>
        <errorMessage>Bitte eine KampagnenID eintragen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_EmptyChannelName</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(MT_SenderName__c)</errorConditionFormula>
        <errorDisplayField>MT_SenderName__c</errorDisplayField>
        <errorMessage>Bitte einen Sendernamen eintragen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_EmptyChannelNumber</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(MT_SenderNumber__c)</errorConditionFormula>
        <errorDisplayField>MT_SenderNumber__c</errorDisplayField>
        <errorMessage>Bitte eine Sendernummer eintragen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_EmptyMonth</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(MT_Month__c), ISNULL(MT_Month__c))</errorConditionFormula>
        <errorDisplayField>MT_Month__c</errorDisplayField>
        <errorMessage>Bitte einen Monat eingeben.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_EmptyNetVolume</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(MT_MN3__c),ISNULL(MT_MN3__c), MT_MN3__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>MT_MN3__c</errorDisplayField>
        <errorMessage>Bitte einen Wert &gt;=0 eintragen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_EmptyProSaOrderId</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(MT_ProSaOrderID__c), ISNULL(MT_ProSaOrderID__c))</errorConditionFormula>
        <errorDisplayField>MT_ProSaOrderID__c</errorDisplayField>
        <errorMessage>Bitte eine ProSa Auftragsnummer hinzufügen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_InvalidMB1Volume</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(MT_MB1__c)), MT_MB1__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>MT_MB1__c</errorDisplayField>
        <errorMessage>Bitte einen Wert &gt;=0 eintragen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_InvalidMB3Volume</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(MT_MB3__c)), MT_MB3__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>MT_MB3__c</errorDisplayField>
        <errorMessage>Bitte einen Wert &gt;=0 eintragen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_InvalidMonth</fullName>
        <active>true</active>
        <errorConditionFormula>YEAR(MT_Month__c) &lt; 2015 || YEAR(MT_Month__c) &gt; 2099</errorConditionFormula>
        <errorDisplayField>MT_Month__c</errorDisplayField>
        <errorMessage>Das Jahr liegt außerhalb des zulässigen Bereichs 2015 - 2099.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BC_ImportData_InvalidTimeslot</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL(MT_Timeslot__c, &apos;1.[06-09]&apos;),ISPICKVAL(MT_Timeslot__c, &apos;2.[09-13]&apos;),ISPICKVAL(MT_Timeslot__c, &apos;3.[13-17]&apos;),ISPICKVAL(MT_Timeslot__c, &apos;4.[17-20]&apos;),ISPICKVAL(MT_Timeslot__c, &apos;5.[20-23]&apos;),ISPICKVAL(MT_Timeslot__c, &apos;6.[23-01]&apos;),ISPICKVAL(MT_Timeslot__c, &apos;7.[01-06]&apos;)))</errorConditionFormula>
        <errorDisplayField>MT_Timeslot__c</errorDisplayField>
        <errorMessage>Unbekannte Zeitschiene</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>MT_StartTransformation</fullName>
        <availability>online</availability>
        <description>Will trigger the transformation batch job.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Start Transformation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/apex.js&quot;)}

const result = sforce.apex.executeAnonymous(&quot;Database.executeBatch(new MT_BroadcastImportDataTransformator());&quot;);

if (result &amp;&amp; result.success) {
  alert(&apos;Transformation successfully started.&apos;);
} else if (!result) {
  alert(&apos;Failed to start data transformation.&apos;);
  if (console &amp;&amp; console.error) {
    console.error(&apos;sforce.apex.executeAnonymous yielded an empty response&apos;);
  }
} else {
  if (console &amp;&amp; console.error) {
    console.error(result);
  }

  const error = result.exceptionMessage || result.compileProblem;
  alert(&apos;Failed to start data transformation (&apos; + error + &apos;)&apos;);
}</url>
    </webLinks>
</CustomObject>
